<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet author="jcx" id="t_amp_attach_entity_rel-1.0.0">

        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="t_amp_attach_entity_rel"/>
            </not>
        </preConditions>

        <createTable tableName="t_amp_attach_entity_rel" remarks="附件与其他表关系">
            <column name="wid" type="VARCHAR(64)" remarks="业务主键">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="attach_wid" type="VARCHAR(64)" remarks="附件wid"/>
            <column name="entity_wid" type="VARCHAR(64)" remarks="其他表wid" />
            <column name="create_time" type="TIMESTAMP" remarks="创建时间" />
            <column name="update_time" type="TIMESTAMP" remarks="更新时间" />

        </createTable>

    </changeSet>

</databaseChangeLog>