{
  "name": "master",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development RUN_MODE=shell vue-cli-service serve",
    "dev:shell": "cross-env NODE_ENV=development RUN_MODE=shell vue-cli-service serve",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "serve": "serve -l 8001 ./dist",
